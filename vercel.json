{
  "version": 2,
  "builds": [
    {
      "src": "django_test/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "buildCommand": "python manage.py migrate && python manage.py collectstatic --noinput",
  "routes": [
    {
      "src": "/static/(.)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.)",
      "dest": "django_test/wsgi.py"
    }
  ]
}
